{
    "name": "Rust Dev Container",
    "image": "mcr.microsoft.com/devcontainers/rust:latest",
    "features": {
        "rust": "latest"
    },
    "customizations": {
        "extensions": [
            "rust-lang.rust-analyzer"
        ]
    },
    "mounts": [
        "source=${env:HOME}/.ssh,target=/home/vscode/.ssh,type=bind,consistency=cached"
    ]
}